{
  "_version": "1.59.0",
  "sap.app": {
    "id": "file",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.14.2",
      "toolsId": "a67fdbcd-b8ae-4f23-a2c3-1cde5733da9a"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/attachments/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.128.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "file.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "HospitalList",
          "target": "HospitalList"
        },
        {
          "pattern": "Hospital({key}):?query:",
          "name": "HospitalObjectPage",
          "target": "HospitalObjectPage"
        },
        {
          "pattern": "Hospital({key})/Files({key2}):?query:",
          "name": "FilesObjectPage",
          "target": "FilesObjectPage"
        }
      ],
      "targets": {
        "HospitalList": {
          "type": "Component",
          "id": "HospitalList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Hospital",
              "variantManagement": "Page",
              "navigation": {
                "Hospital": {
                  "detail": {
                    "route": "HospitalObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              }
            }
          }
        },
        "HospitalObjectPage": {
          "type": "Component",
          "id": "HospitalObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Hospital",
              "navigation": {
                "Files": {
                  "detail": {
                    "route": "FilesObjectPage"
                  }
                }
              },
              "content": {
                "header": {
                  "actions": {
                    "file": {
                      "press": "file.ext.file.file.file",
                      "visible": true,
                      "enabled": true,
                      "requiresSelection": false,
                      "text": "multi-upload"
                    }
                  }
                }
              }
            }
          }
        },
        "FilesObjectPage": {
          "type": "Component",
          "id": "FilesObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Hospital/Files"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
